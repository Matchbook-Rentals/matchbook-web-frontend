{
  "name": "matchbook-com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:lan": "next dev --hostname 0.0.0.0",
    "postinstall": "prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "redis:up": "docker compose -f redis/docker-compose.yml up -d",
    "redis:down": "docker compose -f redis/docker-compose.yml down",
    "ws:dev": "npm run redis:up && node ts_server/server.js",
    "ws:dev:lan": "npm run redis:up && SOCKET_IO_HOST=0.0.0.0 node ts_server/server.js",
    "ws:start": "node ts_server/server.js",
    "ws:start:js": "node ts_server/server.js",
    "ws:deploy": "npm install && npm run ws:start:js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:unit:watch": "vitest --config vitest.unit.config.ts",
    "test:integration": "vitest run test/integration",
    "test:integration:watch": "vitest test/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:video": "RECORD_VIDEO=always playwright test",
    "test:e2e:video:headed": "RECORD_VIDEO=always playwright test --headed",
    "test:e2e:codegen": "playwright codegen localhost:3000",
    "migrate:reservation-deposit": "node scripts/migrate-reservation-deposit.js",
    "verify:reservation-deposit": "node scripts/verify-reservation-deposit-migration.js",
    "cleanup:reservation-deposit": "node scripts/cleanup-reservation-deposit.js"
  },
