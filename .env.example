# API Keys (Required to enable respective provider)
ANTHROPIC_API_KEY="your_anthropic_api_key_here"       # Required: Format: sk-ant-api03-...
PERPLEXITY_API_KEY="your_perplexity_api_key_here"     # Optional: Format: pplx-...
OPENAI_API_KEY="your_openai_api_key_here"             # Optional, for OpenAI/OpenRouter models. Format: sk-proj-...
GOOGLE_API_KEY="your_google_api_key_here"             # Optional, for Google Gemini models.
MISTRAL_API_KEY="your_mistral_key_here"               # Optional, for Mistral AI models.
XAI_API_KEY="YOUR_XAI_KEY_HERE"                       # Optional, for xAI AI models.
AZURE_OPENAI_API_KEY="your_azure_key_here"            # Optional, for Azure OpenAI models (requires endpoint in .taskmasterconfig).
OLLAMA_API_KEY="your_ollama_api_key_here"             # Optional: For remote Ollama servers that require authentication.

# DigitalOcean Spaces Configuration
DIGITALOCEAN_SPACES_KEY="your_do_spaces_key_here"     # Required: DigitalOcean Spaces access key
DIGITALOCEAN_SPACES_SECRET="your_do_spaces_secret_here" # Required: DigitalOcean Spaces secret key
DIGITALOCEAN_SPACES_ENDPOINT="https://atl1.digitaloceanspaces.com" # Required: DigitalOcean Spaces endpoint
DIGITALOCEAN_SPACES_BUCKET="matchbook-rentals"        # Required: DigitalOcean Spaces bucket name

# Medallion/Authenticate.com Configuration (LOW_CODE_SDK integration)
NEXT_PUBLIC_MEDALLION_LOW_CODE_SDK_KEY="your_low_code_sdk_key_here" # Required: Medallion LOW_CODE_SDK_KEY for client-side verification
MEDALLION_WEBHOOK_SECRET="your_webhook_secret_here"  # Optional: Webhook signature verification secret
NEXT_PUBLIC_BASE_URL="https://yourdomain.com"        # Required: Base URL for verification redirects

# API key for webhook user lookup (used to call /user/summary endpoint)
MEDALLION_API_KEY="your_medallion_api_key_here"       # Required: For webhook user matching via /user/summary endpoint

# Email Queue Configuration (Java Worker)
REDIS_URL="redis://localhost:6380"                    # Required: Redis connection URL for email queue (port 6380 to avoid conflict with existing Redis)
USE_EMAIL_QUEUE="false"                               # Optional: Enable email queue (true) or send directly (false). Default: false
RESEND_API_KEY="your_resend_api_key_here"             # Required: Resend API key for email sending